<script type="text/javascript">
youtubeDataMove = () => {
    let temp = [];
    let obj = {};
    let title, rank, lastWeekRank, albumImg, singer, chartDuration, views, link;
    '<% for(let i = 0; i < result.youtubedata.data.length; i++) { %>'
        title = '<%= result.youtubedata.data[i].title %>';
        rank = '<%= result.youtubedata.data[i].rank %>';
        lastWeekRank = '<%= result.youtubedata.data[i].lastWeekRank %>';
        albumImg = '<%= result.youtubedata.data[i].albumImg %>';
        singer = '<%= result.youtubedata.data[i].singer %>';
        chartDuration = '<%= result.youtubedata.data[i].chartDuration %>';
        views = '<%= result.youtubedata.data[i].views %>';
        link = '<%= result.youtubedata.data[i].link %>';

        obj = {
            "title": title,
            "rank": rank,
            "lastWeekRank": lastWeekRank,
            "albumImg": albumImg,
            "singer": singer,
            "chartDuration": chartDuration,
            "views": views,
            "link": link,
        }
        temp.push(obj);
    '<% } %>'

    // console.log(temp);
    return temp;
}

