<script type="text/javascript">
    genieMovieDataMove = () => {
        let temp = [];
        let obj = {};
        let title, rank, rankVariance, albumImg, singer, albumTitle, totalNumPlay;
        '<% for(let i = 0; i < result.genieMoviedata.data.length; i++) { %>'
            title = '<%= result.genieMoviedata.data[i].title %>';
            rank = '<%= result.genieMoviedata.data[i].rank %>';
            rankVariance = '<%= result.genieMoviedata.data[i].rankVariance %>';
            albumImg = '<%= result.genieMoviedata.data[i].albumImg %>';
            singer = '<%= result.genieMoviedata.data[i].singer %>';
            albumTitle = '<%= result.genieMoviedata.data[i].albumTitle %>';
            totalNumPlay = '<%= result.genieMoviedata.data[i].totalNumPlay %>';
    
            obj = {
                "title": title,
                "rank": rank,
                "rankVariance": rankVariance,
                "albumImg": albumImg,
                "singer": singer,
                "albumTitle": albumTitle,
                "totalNumPlay": totalNumPlay
            }
            temp.push(obj);
        '<% } %>'
    
        // console.log(temp);
        return temp;
    }

    
    // ******처음 출력을 위한 초기 실행 함수******
